            <div class="change_language">
                {% set current_path = app.request.get('_route') %}
                {% set current_locale = app.request.get('_locale') %}

                {% if current_locale == 'fr' %}
                    <a href="{{ path(current_path, {'_locale': 'en'}) }}">{% trans %}app.change_locale.en{% endtrans %}</a>
                {% endif %}

                {% if current_locale == 'en' %}
                    <a href="{{ path(current_path, {'_locale': 'fr'}) }}">{% trans %}app.change_locale.fr{% endtrans %}</a>
                {% endif %}
            </div>
        </header>

        <main>
    	{% block body %}{% endblock %}
        </main>

    	<footer>
			<h1>{% trans %}app.footer.who_are_we.titre{% endtrans %}</h1>
			<div>{% trans %}app.footer.who_are_we.description{% endtrans %}</div>
    	</footer>

